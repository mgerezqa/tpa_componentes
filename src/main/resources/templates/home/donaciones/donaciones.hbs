                                    </div>
                                    <div class="col-12 col-md-4">
                                        <div class="tag_info w-100"><i class="bi bi-arrow-repeat"></i> Motivo: <span class="valor_info">{{this.motivo}}</span></div>
                                    </div>
                                {{/if}}
                                {{#if (eq this.getTipo "MantenerHeladera")}}
